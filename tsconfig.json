{
    "compilerOptions": {
        "esModuleInterop": true,
        "module": "Node16",
        "moduleResolution": "Node16",
        "target": "ESNext",
        "strict": true,
        "baseUrl": "./",
        "noImplicitAny": false,
        "rootDir": "./",
        "outDir": "dist",
        "strictNullChecks": true,
        "skipLibCheck": true,
        "strictFunctionTypes": true,
    },
    "exclude": [
        "node_modules",
    ]
}
